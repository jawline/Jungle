function JungleSound() {

	if (!buzz.isSupported()) {
		throw "No Audio Support";
	}

	this.background = null;
}

JungleSound.prototype.setBackground = function() {
	this.background = new buzz.sound("sounds/login", {
		formats : ["mp3", "ogg"],
		preload : true,
		autoplay : true,
		loop : true
	});
}
